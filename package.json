{
  "name": "mycomap-server",
  "type": "module",
  "imports": {
    "#*": "./*.js"
  },
  "scripts": {
    "start": "node --env-file=.env server.js",
    "dev": "node --watch --env-file=.env server.js",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js",
    "db:schema": "psql -d mycomapdb -f db/schema.sql",
    "db:seed": "node --env-file=.env db/seed.js",
    "db:reset": "npm run db:schema && npm run db:seed"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.921.0",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "multer-s3": "^3.0.1",
    "pg": "^8.14.1"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "jest": "^30.1.3",
    "supertest": "^7.1.4",
    "vitest": "^3.1.3"
  }
}
